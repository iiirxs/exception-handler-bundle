{
    "name": "iiirxs/exception-handler-bundle",
    "description": "A Symfony bundle that provides custom exception handling out of the box",
    "type": "symfony-bundle",
    "require": {
        "ext-json": "*",
        "php": "^7.1.3",
        "symfony/config": "^4.0",
        "symfony/dependency-injection": "^4.0",
        "symfony/event-dispatcher": "^4.0",
        "symfony/http-kernel": "^4.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nik G",
            "email": "iiirxs@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "IIIRxs\\ExceptionHandlerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IIIRxs\\ExceptionHandlerBundle\\Tests\\": "tests/"
        }
    }
}
